      "source": [
        "#for文を使う リストを作成\n",
        "list = [\"水\", \"金\", \"地\", \"火\", \"木\", \"土\", \"天\", \"海\", \"冥\"]\n",
        "\n",
        "# 値を出力\n",
        "for planet in list:\n",
        "     print(planet)\n",
        "\n",
        "\n",
        "# while文を使って　リストを作成\n",
        "list = [\"水\", \"金\", \"地\", \"火\", \"木\", \"土\", \"天\", \"海\", \"冥\"]\n",
        "\n",
        "i = 0\n",
        "while i < 1:\n",
        "  for p in list:\n",
        "   print(p)\n",
        "\n",
        "  i = i + 1\n",
        "\n"
      ]
